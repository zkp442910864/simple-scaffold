        "js-yaml": "^4.1.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^7.0.1",
        "tree-kill": "^1.2.2",
        "zustand": "^5.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.11.1",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^22.10.1",
