{
  "name": "vite-shopify",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "run-p -sr \"shopify:dev -- {@}\" \"vite:dev\" --",
    "deploy": "run-s \"vite:build\" \"shopify:push -- {@}\" --",
    "shopify:dev": "shopify theme dev",
    "shopify:push": "shopify theme push",
    "unocss:dev": "unocss ./{snippets,frontend}/**/* -o ./assets/uno.css -w",
    "unocss:build": "unocss ./{snippets,frontend}/**/* -o ./assets/uno.css -m",
    "vite:dev": "vite",
    "vite:build": "vite build"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@by-association-only/vite-plugin-shopify-clean": "^2.0.0",
    "@eslint/js": "^9.11.1",
    "@shopify/cli-kit": "^3.70.0",
    "@shopify/plugin-cloudflare": "^3.70.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@unocss/cli": "^0.64.0",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "debug": "^4.3.7",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "fast-glob": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "sass-embedded": "^1.80.7",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "unocss": "^0.64.0",
    "vite": "^5.4.8",
    "vite-plugin-shopify": "^3.1.0",
    "vite-plugin-shopify-import-maps": "^0.5.4"
  }
}
